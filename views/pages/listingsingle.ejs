<div class="top-button-container">
    <div class="large-button left text-center">
        <a class="large-button-inner" href="/listings/">&lt; Back to listings</a>
    </div>
</div>
<% if(listing) { %>
<div class="page-title" style="text-align: center;">
    <span style="font-family: sans-serif; font-size: 16px">Listing ID </span>
    <span style="font-family: monospace; font-size: 16px"><%= listing.uuid %></span>
</div>
<div class="listing-properties" style="text-align: center;">
    <p style="font-family: monospace">
        <%= JSON.stringify(listing, null, 2)%>
    </p>
</div>
<% } else { %>
<div style="text-align: center;"><h1>ERROR: Listing could not be displayed</h1></div>
<% } %>