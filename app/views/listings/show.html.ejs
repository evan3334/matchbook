<div class="hero-unit">
  <h2 class="title"><%= listing.title %></h2>
  <%- linkTo('Edit this listing', editListingPath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', listingPath(''), {class: 'btn pull-right'}); %>
</div>

<h2>Listing Properties</h2>
<% for(var i in listing) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= listing[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>